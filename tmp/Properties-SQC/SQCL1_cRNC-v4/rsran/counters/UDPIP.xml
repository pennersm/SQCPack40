<?xml version="1.0" encoding="UTF-8"?>
<conf_file>
  <info>
    <release number="jflx" />
    <dst />
    <implementation />
    <min_obj_agg />
    <maxrows>100</maxrows>
    <header>Nokia Siemens RNC RN60_1 UDP_meas_on_IP UDPIP measurement dump</header>
    <csv_title>Nokia Siemens RNC RN60_1 UDP_meas_on_IP UDPIP measurement dump</csv_title>
    <report-description>UDP measurement on IP interface</report-description>
    <obj_number>20</obj_number>
    <csv_rule>reuse</csv_rule>
    <addon_sorting />
    <wrap_header>off</wrap_header>
    <help_file>about_kpi_help.htm</help_file>
    <nokia_counter_id_help_file />
    <nokia_counter_db_help_file />
    <external_kpi_help_file />
  </info>
  <column_order>
    <period_duration />
    <IN_DATAGRAMS />
    <NO_PORTS />
    <IN_ERRORS />
    <OUT_DATAGRAMS />
    <period_duration_sum />
  </column_order>
  <temp_table name="UDPIP" meas_level="PLMN/RNC/UTYPE/UNITID/IPIF" type="PM" master="yes" pragma="oss4ran" create="yes">
    <counters>
      <counter alias="period_duration">AVG(period_duration)*COUNT(DISTINCT period_start_time)</counter>
      <counter alias="IN_DATAGRAMS">SUM(IN_DATAGRAMS)</counter>
      <counter alias="NO_PORTS">SUM(NO_PORTS)</counter>
      <counter alias="IN_ERRORS">SUM(IN_ERRORS)</counter>
      <counter alias="OUT_DATAGRAMS">SUM(OUT_DATAGRAMS)</counter>
      <counter alias="period_duration_sum">SUM(period_duration_sum)</counter>
    </counters>
    <table_name>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</table_name>
    <alias_name>UDPIP</alias_name>
  </temp_table>
  <kpi name="period_duration">
    <formula>UDPIP.period_duration</formula>
    <kpi_info>
      <title>period_duration</title>
      <use>Duration of the measurement period in minutes.</use>
      <open_questions />
      <use_experience />
      <known_problems />
      <pm_class />
      <formula>AVG(period_duration)*COUNT(DISTINCT period_start_time)</formula>
      <unit />
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>period_duration</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark />
    <area_level />
  </kpi>
  <kpi name="IN_DATAGRAMS">
    <formula>UDPIP.IN_DATAGRAMS</formula>
    <kpi_info>
      <title>IN_DATAGRAMS</title>
      <use>The number of UDP datagrams received. UPDATED:When a UDP datagram is received.</use>
      <open_questions />
      <use_experience />
      <known_problems />
      <pm_class />
      <formula>SUM(IN_DATAGRAMS)</formula>
      <unit>Integer number</unit>
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>IN_DATAGRAMS (M566C0)</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark />
    <area_level />
  </kpi>
  <kpi name="NO_PORTS">
    <formula>UDPIP.NO_PORTS</formula>
    <kpi_info>
      <title>NO_PORTS</title>
      <use>The number of UDP datagrams for which there is no application at the destination port. UPDATED: When a UDP datagram, for which there is no application listening at the destination UDP port, is received by the interface unit.</use>
      <open_questions />
      <use_experience />
      <known_problems />
      <pm_class />
      <formula>SUM(NO_PORTS)</formula>
      <unit>Integer number</unit>
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>NO_PORTS (M566C1)</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark />
    <area_level />
  </kpi>
  <kpi name="IN_ERRORS">
    <formula>UDPIP.IN_ERRORS</formula>
    <kpi_info>
      <title>IN_ERRORS</title>
      <use>The number of UDP datagrams received with errors. UPDATED:When a UDP datagram with errors is received.</use>
      <open_questions />
      <use_experience />
      <known_problems />
      <pm_class />
      <formula>SUM(IN_ERRORS)</formula>
      <unit>Integer number</unit>
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>IN_ERRORS (M566C2)</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark />
    <area_level />
  </kpi>
  <kpi name="OUT_DATAGRAMS">
    <formula>UDPIP.OUT_DATAGRAMS</formula>
    <kpi_info>
      <title>OUT_DATAGRAMS</title>
      <use>The number of UDP datagrams locally sent. The datagrams generated by the host processor are not included. UPDATED:When a UDP datagram is sent.</use>
      <open_questions />
      <use_experience />
      <known_problems />
      <pm_class />
      <formula>SUM(OUT_DATAGRAMS)</formula>
      <unit>Integer number</unit>
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>OUT_DATAGRAMS (M566C3)</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark />
    <area_level />
  </kpi>
  <kpi name="period_duration_sum">
    <formula>UDPIP.period_duration_sum</formula>
    <kpi_info>
      <title>period_duration_sum</title>
      <use />
      <open_questions />
      <use_experience />
      <known_problems />
      <pm_class />
      <formula>SUM(period_duration_sum)</formula>
      <unit>Min</unit>
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>period_duration_sum</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark />
    <area_level />
  </kpi>
</conf_file>

