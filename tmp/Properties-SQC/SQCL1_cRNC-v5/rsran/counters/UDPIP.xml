          <counter alias="NO_PORTS" symmetrical="true">SUM(NO_PORTS)</counter>
          <counter alias="IN_ERRORS" symmetrical="true">SUM(IN_ERRORS)</counter>
          <counter alias="OUT_DATAGRAMS" symmetrical="true">SUM(OUT_DATAGRAMS)</counter>
          <counter alias="period_duration_sum" symmetrical="true">SUM(period_duration_sum)</counter>
