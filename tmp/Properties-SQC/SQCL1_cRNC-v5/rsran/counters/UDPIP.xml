      <unit>min</unit>
      <tables>nokrww_ps_udpip_{OBJ_AGG}_{TIME_AGG}</tables>
    </kpi_info>
    <kpi_alias>period_duration (period_duration)</kpi_alias>
    <kpi_format>float:2</kpi_format>
    <kpi_mark/>
    <area_level/>
